{
  "name": "9gag webp remover",
  "version": "1.0",
  "description": "Removes all webp in front of img tags!",
  "content_scripts": [
    {
        "matches": ["http://9gag.com/", "http://9gag.com/*", "https://9gag.com/", "https://9gag.com/*"],
        "js": ["/content_script.js"]
    }
  ],
  "icons": {
        "2000": "9gag.png"
    },
  "manifest_version": 2
}
